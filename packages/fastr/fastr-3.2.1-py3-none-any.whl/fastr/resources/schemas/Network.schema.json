{

    "type": "object",
    "class": "fastr.planning.network.Network",
    "$schema": "http://json-schema.org/draft-04/schema",
    "required": ["id", "nodelist", "linklist", "node_number", "link_number"],
    "definitions": {},
    "properties": {
        "id": {
            "type": "string"
        },
        "version": {
            "$ref": "fastr:///common.schema.json#/definitions/version"
        },
        "description": {
            "type": "string"
        },
        "node_number": {
            "type": "integer"
        },
        "link_number": {
            "type": "integer"
        },
        "nodelist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "Node.schema.json#/"}
        },
        "linklist": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"$ref": "Link.schema.json#/"}
        },
        "preferred_types": {
            "type": "array",
            "uniqueItems": "true",
            "items": {"type": "string"}
        },
        "nodegroups": {
            "type": "object",
            "patternProperties": {
                ".*": {
                    "type": "array",
                    "uniqueItems": "true",
                    "items": {"type": "string"}
                }
            }
        },
        "stepids": {
            "type": ["object", "null"],
            "default": {},
            "patternProperties": {
                ".*": {
                    "type": "array",
                    "uniqueItems": "true",
                    "items": {"type": "string"}
                }
            }
        }
    }
}