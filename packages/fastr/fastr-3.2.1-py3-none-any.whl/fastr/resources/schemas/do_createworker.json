{
    "type": "object",
    "$schema": "http://json-schema.org/draft-04/schema",
    "required": ["name", "user"],
    "description": "RPC specification for do_createworker",
    "properties": {
        "name": {
            "type": "string",
            "description": "Name of the worker process to created"
        },
        "user": {
            "type": "string",
            "description": "Username for the system user that will be the process owner"
        }
    }
}