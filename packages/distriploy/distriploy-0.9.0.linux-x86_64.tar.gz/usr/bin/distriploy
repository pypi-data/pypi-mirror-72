#!/usr/lib/python-exec/python3.7/python
# EASY-INSTALL-ENTRY-SCRIPT: 'distriploy==0.9.0','console_scripts','distriploy'
__requires__ = 'distriploy==0.9.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('distriploy==0.9.0', 'console_scripts', 'distriploy')()
    )
