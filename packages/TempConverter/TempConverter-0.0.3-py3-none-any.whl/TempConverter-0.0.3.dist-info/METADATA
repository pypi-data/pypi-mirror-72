import converter.converts as cd
n = 20

print(cd.c2f(n)) ## Celsius to fahrenheit
print(cd.f2c(n)) ## Fahrenheit to Celsius
print(cd.add_temp(n)) ## Adds both Celsius and Fahrenheit values

